html 
  scroll-behavior: initial

html, body 
  position relative
  width: 100%
  min-height: 100%

::before, ::after
  position absolute
  content ""

.section_title-outline 
  position: relative
  -webkit-text-fill-color: transparent
  -webkit-text-stroke: 2px white !important


.section_title-outline::after 
  /* Place a second copy of the same text over top */
  content attr(data-text)
  position absolute
  left 0
  -webkit-text-stroke 0
  pointer-events none


.section_title-leaf 
  position relative
  -webkit-text-fill-color transparent
  -webkit-background-clip text



.contact_bg_mask 
  -webkit-mask-image url(https://uploads-ssl.webflow.com/636a3cc0e8123382bf004c5d/63734d310109ae243167976d_logo-big.svg)
  -webkit-mask-repeat no-repeat
  -webkit-mask-position center
  -webkit-mask-size 50%

.hero_navbar
  padding-top @css{min(8vh, 90px)}
.hero_info
  padding-top @css{min(15vh, 390px)}
.hero_cta
  padding-top @css{min(9vh, 390px)}
.contact_bg_video 
  width 100vw
  height 100vh

.tt
  display none

